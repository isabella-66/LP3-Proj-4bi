@model LP3_ProjetoFinal.Models.MovieTheater;

@{
    ViewData["Title"] = $"Criar Nova Sala";
}
<h1>@ViewData["Title"]</h1>

<form id="form" action="Create" method="post">
    <p>
        <label for="number">Id: </label>
        <input type="number" id="id" name="id">
    </p>

    <p>
        <label for="number">Número: </label>
        <input type="number"  id="number" name="number">
    </p>

    <p>
        <label for="numberSeats">Número de assentos: </label>
        <input type="number" id="numberSeats" name="numberSeats">
    </p>

    <p>
        <label for="hall">Corredor: </label>
        <input type="text" id="hall" name="hall">
    </p>

    <button type="submit">Salvar</button>
</form>